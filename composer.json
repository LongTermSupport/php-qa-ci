{
  "name": "edmondscommerce/phpqa",
  "license": "MIT",
  "type": "project",
  "description": "Simple PHP QA pipeline and scripts. Largely just a collection of dependencies with configuration and scripts to run them together",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\PHPQA\\": [
        "src/",
        "tests/"
      ]
    }
  },
  "require": {
    "phpunit/phpunit": "^6.2",
    "phpstan/phpstan": "^0.8.0",
    "squizlabs/php_codesniffer": "3.*",
    "escapestudios/symfony2-coding-standard": "3.x-dev",
    "friendsofphp/php-cs-fixer": "*@stable",
    "jakub-onderka/php-parallel-lint": "*@stable",
    "jakub-onderka/php-console-highlighter": "*@stable",
    "sllh/composer-versions-check": "*@stable",
    "johnkary/phpunit-speedtrap": "dev-master",
  },
  "bin": [
    "bin/phpqa"
  ],
  "require-dev": {
  },
  "config": {
    "bin-dir": "bin",
    "secure-http": false,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
