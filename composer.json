{
  "name": "edmondscommerce/phpqa",
  "license": "MIT",
  "type": "project",
  "description": "Simple PHP QA pipeline and scripts. Largely just a collection of dependencies with configuration and scripts to run them together",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\PHPQA\\": [
        "src/",
        "tests/"
      ]
    }
  },
  "require": {
    "phpunit/phpunit": "~6.4.0",
    "phpstan/phpstan": "^0.9",
    "squizlabs/php_codesniffer": "^3.2",
    "jakub-onderka/php-parallel-lint": "^0.9",
    "jakub-onderka/php-console-highlighter": "^0.3",
    "johnkary/phpunit-speedtrap": "^2.0",
    "phploc/phploc": "^4.0",
    "phpmd/phpmd": "~2",
    "phpstan/phpstan-strict-rules": "^0.9.0"
  },
  "bin": [
    "bin/qa"
  ],
  "require-dev": {
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
